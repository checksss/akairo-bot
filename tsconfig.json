{
    "compilerOptions": {
        "target": "es2017",
        "strict": true,
        "moduleResolution": "node",
        "declaration": false,
        "removeComments": false,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "alwaysStrict": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "pretty": true,
        "outDir": "./dist",
        "module": "commonjs"
    },
    "include": [
        "client/*",
        "commands/**/*",
        "inhibitors/**/*",
        "listeners/**/*",
        "structures/**/*",
        "bot.ts"
    ],
    "exclude": [
        "node_modules/*"
    ]
}